{
  "name": "levenyatko/auto-applied-coupons",
  "description": "The Auto Applied Coupons plugin is a great solution for WooCommerce sites with a large number of coupons.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0+",
  "require": {
    "composer/installers": "v2.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "wp-coding-standards/wpcs": "^3.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
